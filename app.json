{
  "expo": {
    "name": "carta-menu-app-expo-android",
    "slug": "carta-menu-app-expo-android",
    "version": "0.8.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cartamenuappexpoandroid",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      // ID de paquete ÃšNICO y correcto
      "package": "com.dpasamar.cartamenuappexpoandroid",
      "config": {
        "googleMaps": {
          // API
          "apiKey": "AIzaSyBn-HdnT4lqC0poBl0et0vzep1xxJPBrCY"
        }
      },
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eb2de255-ce9f-43bf-95fc-fa36bd4c61ff"
      }
    }
  }
}
